        "b2",
        "storage",
        "backblaze",
        "cloud",
        "filesystem",
        "backup"
    ],
    "homepage": "https://github.com/zaxbux/backblaze-b2-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Zachary Schneider",
            "email": "hello@zacharyschneider.ca",
            "homepage": "https://www.zacharyschneider.ca"
        }
    ],
    "require": {
